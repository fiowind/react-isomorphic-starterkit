    "stylus-loader": "^2.3.1",
    "supervisor": "^0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
